{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["**spec**"],
  "rules": {
    "no-use-before-define": "off", 
    "import/prefer-default-export": "off",
    "max-classes-per-file": "off",
    "import/extensions": ["error", "ignorePackages", {"js": "always"}],
    "prefer-arrow-callback": 0,
    "linebreak-style": 0,
	 "no-tabs": "off",
	 "indent": "off",
	 "no-plusplus": "off",
	 "no-underscore-dangle": "off"
  },
  "extends": ["airbnb"],
  "settings": {
    "react": {
      "version": "."
    }
  }
}
